call init_strategy_Aboms()
 endif
 endif
 else
if strategy < 11 then
if strategy < 10 then
call init_strategy_Fiends()
 else
call init_strategy_MeatFiends()
 endif
 else
if strategy < 12 then
call init_strategy_Necro()
 else
call init_strategy_Ghouls()
 endif
 endif
 endif
 endif

endfunction

