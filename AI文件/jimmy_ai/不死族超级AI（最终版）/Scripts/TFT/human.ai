function init_strategy takes nothing returns nothing
  call InitBlockList()
  call InitRBlockList()
  call InitHarass()
  call global_init_strategy()
  set exp_first_time = p_exp_first_time[profile] + strategy_first_exp_time[strategy]
  set exp_second_time = p_exp_second_time[profile] + strategy_second_exp_time[strategy]
  call ApplyStrategyRaceAggression()

if strategy < 6 then
if strategy < 2 then
if strategy < 1 then
call init_strategy_Rifle()
 else
call init_strategy_NormalHuman()
 endif
 else
if strategy < 5 then
if strategy < 4 then
if strategy < 3 then
call init_strategy_Copters()
 else
call init_strategy_HAntiTower()
 endif
 else
call init_strategy_MassSB()
 endif
 else
call init_strategy_SanctumMix()
 endif
 endif
 else
if strategy < 9 then
if strategy < 7 then
call init_strategy_BarrackMix()
 else
if strategy < 8 then
call init_strategy_Mortars()
 else
call init_strategy_Dragonhawk()
 endif
 endif
 else
if strategy < 11 then
if strategy < 10 then
call init_strategy_MassSp()
 else
call init_strategy_VersusOrc()
 endif
 else
if strategy < 12 then
call init_strategy_Gryph()
 else
call init_strategy_Knight()
 endif
 endif
 endif
 endif

endfunction

